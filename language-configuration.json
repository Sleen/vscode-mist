{
	"comments": {
		"lineComment": "//",
		"blockComment": [ "/*", "*/" ]
	},
	"brackets": [
		["{", "}"],
		["[", "]"]
	],
	"autoClosingPairs": [
		{ "open": "{", "close": "}", "notIn": ["comment"] },
		{ "open": "[", "close": "]", "notIn": ["comment"] },
		{ "open": "(", "close": ")", "notIn": ["comment"] },
		{ "open": "\"", "close": "\"", "notIn": ["string", "comment"] },
		{ "open": "/*", "close": "*/", "notIn": ["string", "comment"] }
	],
	"autoCloseBefore": ";:.,=}])>< \n\t\"",
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["'", "'"],
		["\"", "\""]
	]
}